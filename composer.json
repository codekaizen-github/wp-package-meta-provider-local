{
    "name": "codekaizen/wp-package-meta-provider-local",
    "authors": [
        {
            "name": "Andrew Dawes",
            "email": "andrewjamesdawes@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "codekaizen/wp-package-meta-provider-contract": "dev-main",
        "psr/log": "^3.0",
        "respect/validation": "^2.2"
    },
    "config": {
        "platform": {
            "php": "8.1.0"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "CodeKaizen\\WPPackageMetaProviderLocal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeKaizen\\WPPackageMetaProviderLocalTests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "analyze": "phpstan"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.0",
        "wp-coding-standards/wpcs": "^3.0"
    }
}
